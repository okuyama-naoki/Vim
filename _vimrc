" NeoCompleteの設定
let g:neocomplete#enable_at_startup = 1
let g:neocomplete#enable_smart_case = 1


" 不要なファイルは作らない
set noswapfile
set nobackup
set viminfo=
set noundofile

" 表示回り
set number
set list
set listchars=tab:>-,trail:-,eol:<
set tabstop=4
set softtabstop=4
set shiftwidth=4
set noexpandtab

